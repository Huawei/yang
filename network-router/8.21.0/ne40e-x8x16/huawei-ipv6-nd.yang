              path "/ifm:ifm/ifm:interfaces/ifm:interface/ifm:name";
            }
            description
              "Interface name.";
          }
          leaf target-address {
            type inet:ipv6-address-no-zone;
            description
              "Target IPv6 address of a miss message.";
          }
