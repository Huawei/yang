                    }
                    container binding-sid {
                      must "(not(../binding-sid/value) or (../binding-sid/value and ../../../../../../../policy-attributes/locator and /ni:network-instance/ni:instances/ni:instance[ni:name='_public_']/srv6:srv6/srv6:locators/srv6:locator[srv6:name=/ni:network-instance/ni:instances/ni:instance[ni:name='_public_']/srv6:srv6/srpolicy:srpolicy/srpolicy:policy-attributes/srpolicy:locator]/srv6:static-length>0))";
                      description "Configure a binding SID.";
                      leaf dataplane {
                        type dataplane-type;
                        must "(../dataplane='srv6')";
                        description
