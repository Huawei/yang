  import huawei-ifm {
    prefix "ifm";
  }
  import huawei-qos {
    prefix "qos";
  }
  import huawei-l2vpn-vll-ext {
    prefix "l2vpn-vll-ext";
  }

  organization
    "Huawei Technologies Co., Ltd.";
  contact
    "Huawei Industrial Base
     Bantian, Longgang
     Shenzhen 518129
     People's Republic of China
     Website: http://www.huawei.com
     Email: support@huawei.com";
