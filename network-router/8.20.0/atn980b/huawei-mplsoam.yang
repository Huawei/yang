        value 1;
        description
          "The VC type is ATM-1to1-VPC.";
      }
      enum "atm-aal5-pdu" {
        value 2;
        description
          "The VC type is atm-aal5-pdu.";
      }
      enum "atm-aal5-sdu" {
        value 3;
        description
          "The VC type is atm-aal5-sdu.";
      }
      enum "atm-nto1-vcc" {
        value 4;
        description
          "The VC type is ATM-nto1-VCC.";
      }
      enum "atm-nto1-vpc" {
        value 5;
        description
          "The VC type is ATM-nto1-VPC.";
