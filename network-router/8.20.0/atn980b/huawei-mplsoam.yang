  typedef detect-interval {
    type enumeration {
      enum "3ms" {
        value 0;
        description
          "Interval value of 3ms.";
      }
      enum "10ms" {
        value 1;
        description
          "Interval value of 10ms.";
      }
      enum "20ms" {
        value 2;
        description
          "Interval value of 20ms.";
      }
      enum "50ms" {
        value 3;
        description
          "Interval value of 50ms.";
      }
      enum "100ms" {
        value 4;
        description
          "Interval value of 100ms.";
      }
      enum "200ms" {
        value 5;
        description
          "Interval value of 200ms.";
      }
      enum "500ms" {
        value 6;
        description
          "Interval value of 500ms.";
      }
      enum "1000ms" {
        value 7;
        description
          "Interval value of 1000ms.";
      }
